
<nav aria-label="Page navigation example">
    <p>{{this.count}}</p>
  <ul class="pagination">
    <li class="page-item">
      <a class="page-link" href="#" aria-label="Previous"{{on "click" (fn this.previousPage)}}>
        <span aria-hidden="true">&laquo;</span>
        <span class="sr-only">Previous</span>
      </a>
    </li>
    <li class="page-item">
      <a class="page-link" href="#" aria-label="Next"{{on "click" (fn this.nextPage)}}>
        <span aria-hidden="true">&raquo;</span>
        <span class="sr-only">Next</span>
      </a>
    </li>
  </ul>
</nav>


<button type="button" class="button"{{on "click" (fn this.changeLoading)}}>+1</button>

<button type="button" class="button"{{on "click" (fn this.changeLoading)}}>-1</button>

  {{#if this.loadingEvent}}

  <Loading />

  {{/if}}



